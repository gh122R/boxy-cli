{
    "name": "boxy/boxy",
    "type": "library",
    "description": "micro cli framework for PHP 8.4+",
    "keywords": ["cli","terminal"],
    "homepage": "https://github.com/Seldaek/monolog",
    "license": "MIT",

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Command\\": "commands/"
        },
        "files": [
            "config/constants.php",
            "helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Арсений Бушуев"
        }
    ],
    "require": {
        "league/climate": "^3.10",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.9",
        "laravel/prompts": "^0.3.6"
    }
}
